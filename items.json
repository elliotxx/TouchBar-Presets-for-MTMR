[
  // ESC Button
  {
    "type": "escape",
    "align": "left",
    "width": 70
  },

  // Battery
  {
    "type": "battery",
    "align": "left",
    "width": 70,
    "image": {
      "base64": ""
    },
    "bordered": false
  },

  // CPU
  {
    "type": "shellScriptTitledButton",
    "width": 70,
    "refreshInterval": 2,
    "source": {
      "inline": "top -l 2 -n 0 -F | egrep -o ' \\d*\\.\\d+% idle' | tail -1 | awk -F% '{p = 100 - $1; if (p > 30) c = \"\\033[33m\"; if (p > 70) c = \"\\033[30;43m\"; printf \"%s%4.1f%%\\n\", c, p}'"
    },
    "action": "appleScript",
    "actionAppleScript": {
      "inline": "activate application \"Activity Monitor\"\rtell application \"System Events\"\r\ttell process \"Activity Monitor\"\r\t\ttell radio button \"CPU\" of radio group 1 of group 2 of toolbar 1 of window 1 to perform action \"AXPress\"\r\tend tell\rend tell"
    },
    "align": "left",
    "image": {
      "base64": ""
    },
    "bordered": false
  },

  // Memory
  {
    "type": "appleScriptTitledButton",
    "width": 70,
    "source": {
      "inline": "set mem to do shell script \"ps -A -o %mem | awk '{s+=$1}END{print s}'\"\rreturn mem & \"%\""
    },
    "action": "appleScript",
    "actionAppleScript": {
      "inline": "activate application \"Activity Monitor\"\rtell application \"System Events\"\r\ttell process \"Activity Monitor\"\r\t\ttell radio button \"CPU\" of radio group 1 of group 2 of toolbar 1 of window 1 to perform action \"AXPress\"\r\tend tell\rend tell"
    },
    "refreshInterval": 5,
    "align": "left",
    "image": {
      "base64": ""
    }
  },

  // Network
  // { "type": "network", "flip": true, "align": "left", "bordered": false },

  // Brightness Control
  {
    "type": "brightnessDown",
    "width": 40,
    "align": "left"
  },
  {
    "type": "brightness",
    "width": 90,
    "align": "left",
    "image": {
      "base64": ""
    }
  },
  {
    "type": "brightnessUp",
    "width": 40,
    "align": "left"
  },

  // Volume Control
  {
    "type": "volumeDown",
    "bordered": true,
    "align": "left",
    "width": 40,
  },
  {
    "type": "volume",
    "width": 90,
    "align": "left",
    "image": {
      // "base64": ""
      "base64": ""
    }
  },
  {
    "type": "volumeUp",
    "bordered": true,
    "align": "left",
    "width": 40
  },

  // Music
  {
    "type": "previous",
    "align": "left",
    "width": 40
  },
  {
    "type": "play",
    "align": "left",
    "width": 40
  },
  {
    "type": "next",
    "align": "left",
    "width": 40
  },

  // Mute Button
  {
    "type": "mute",
    "align": "left",
    "width": 45
  },

  // Lock Screen Button
  {
    "type": "staticButton",
    "title": "",
    "action": "appleScript",
    "actionAppleScript": {
      "inline": "tell application \"ScreenSaverEngine\"\ractivate\rend tell",
    },
    "align": "left",
    "width": 55,
    "image": {
      "base64": ""
    },
  },


  // Time
  {
    "type": "timeButton",
    "formatTemplate": "MM/d HH:mm E",
    "align": "left",
    "bordered": false,
    "longAction": "shellScript",
    "longExecutablePath": "/usr/bin/pmset",
    "longShellArguments": ["sleepnow"]
  }
]